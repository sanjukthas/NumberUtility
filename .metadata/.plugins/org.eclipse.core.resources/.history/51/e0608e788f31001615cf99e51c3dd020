
public class MoreEscapeSequence {
	public static void main(String[] args) {
	
		        System.out.println("Cost \t Quantity \t Total \n");
				System.out.println(" $1,000.00 \t 4 \t $4,000.00 \n");
				System.out.println(" $50.00 \t 8 \t $400.00 \n");
				System.out.println("_____________________________");
			    System.out.println(" TOTAL: \t 12 \t $4,400.00" );
	

}
}
