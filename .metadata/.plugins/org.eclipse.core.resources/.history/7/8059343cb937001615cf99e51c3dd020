
public class Driver extends Person {

	String DriverID;
	String DL;
	
	
	public Driver(String driverID, String dL) {
		super();
		DriverID = driverID;
		DL = dL;
	}
	
	public String getDriverID() {
		return DriverID;
	}
	public void setDriverID(String driverID) {
		DriverID = driverID;
	}
	public String getDL() {
		return DL;
	}
	public void setDL(String dL) {
		DL = dL;
	}
	
	

}
