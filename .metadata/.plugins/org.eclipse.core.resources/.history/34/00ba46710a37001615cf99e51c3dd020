import java.util.Scanner;
 
public class Leap1{
	private int month;
	private int day;
	private int year;
	
	void DisplayDate() {
		
	}
	
	
	void check() {

		System.out.println();
						
		for(int i=0; i<=year; i++) {
			
			if(i%400 == 0 || i%4 == 0) {
				
				System.out.println(i + " is a leap year");			
			}
		}
	}
}