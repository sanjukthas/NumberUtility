
public class Car extends VehicleApp{

	String model ;
	String colour;
	Driver cardriver;
	
	public Car (String model, String colour, String name, String DriverID, String DL) {
		this.model= model;
		this.colour=colour;
		this.cardriver=new Driver(name, DriverID, DL);
	}

	
	public String getModel() {
		return model;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public String getColour() {
		return colour;
	}

	public void setColour(String colour) {
		this.colour = colour;
	}

	public String getCarDriverName() {
		//return cardriver.name;
		return cardriver.getName();
	}

	public String getDriverID() {
		return cardriver.getDriverID();
	}

	public String getDL() {
		return cardriver.getDL();
	}
	public void setCardriver(Driver cardriver) {
		this.cardriver = cardriver;
	}



	public void road (){
		
	System.out.println("The red Porsche is starting \n");
	System.out.println("The red Porsche is accelerating \n");
	}
	
		public long acc() {
		
	System.out.println("The blue Jetta is starting \n");
	System.out.println("The blue Jetta is accelerating \n");
	return 0;
	}
	public void road1 (){
		
    System.out.println("The red Porsche is going 50 mph \n"); 
    System.out.println("The red Porsche is stopped \n");
	
	}
	public void acc1() {
		
	System.out.println("The blue Jetta has stopped to have its emissions checked \n" );
	System.out.println("The blue Jetta passes its emission test \n");
	}

	}