
public class Car extends VehicleApp{

	public void road (){
		
	System.out.println("The red Porsche is starting \n");
	System.out.println("The red Porsche is accelerating \n");
	}
	
	public long driver(){
		System.out.println("The car is driven by the driver");
		return "Driver in car";
	}
	public long acc() {
		
	System.out.println("The blue Jetta is starting \n");
	System.out.println("The blue Jetta is accelerating \n");
	return 0;
	}
	public void road1 (){
		
    System.out.println("The red Porsche is going 50 mph \n"); 
    System.out.println("The red Porsche is stopped \n");
	
	}
	public void acc1() {
		
	System.out.println("The blue Jetta has stopped to have its emissions checked \n" );
	System.out.println("The blue Jetta passes its emission test \n");
	}

	}