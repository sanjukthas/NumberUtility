
public class Fibconj {
	 
	   public static void main(String[] args) {
	      int n = Integer.parseInt(args[0]);
	      collatz(n);
	      System.out.println();
	    }
	 
	   public static void collatz(int n) {
	      System.out.print(n + " ");
	      if (n == 1) {
	         return;
	      } else if (n % 2 == 0) {
	          collatz(n / 2);
	      } else {
	         collatz(3*n + 1);
	      }
	   }
	}
	 